    /* -------------------- */
    /* CSS                  */
    /* -------------------- */
    "[css]": {
        "editor.tabSize": 2,
        "prettier.tabWidth": 2,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    // "css.hover.documentation": false,
    // "css.hover.references": false,

    /* -------------------- */
    /* Tailwind CSS         */
    /* -------------------- */
    "[tailwindcss]": {
        "editor.tabSize": 2,
        "prettier.tabWidth": 2,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "tailwindCSS.emmetCompletions": true,
    "tailwind-fold.foldStyle": "QUOTES",
    "tailwind-fold.unfoldIfLineSelected": true,

    /* -------------------- */
    /* SCSS                 */
    /* -------------------- */
    "[scss]": {
        "editor.tabSize": 4,
        "prettier.tabWidth": 4,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    /* -------------------- */
    /* Go                   */
    /* -------------------- */
    "[go]": {
        "editor.tabSize": 4,
        "editor.detectIndentation": false
    },
    "go.toolsManagement.autoUpdate": true,
    "go.formatTool": "gofumpt",

    /* -------------------- */
    /* Python               */
    /* -------------------- */
    "[python]": {
        "editor.tabSize": 4,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.autoImportCompletions": true,
    "python.analysis.autoFormatStrings": true,
    "python.analysis.inlayHints.callArgumentNames": "all",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.completeFunctionParens": true,

    /* -------------------- */
    /* Lua                  */
    /* -------------------- */
    "[lua]": {
        "editor.tabSize": 4,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "yinfei.luahelper"
    },
    "luahelper.format.indent_width": 4,
    "luahelper.format.continuation_indent_width": 4,
    "luahelper.format.column_limit": 140,
    "luahelper.format.spaces_inside_table_braces": false,
    "luahelper.format.keep_simple_control_block_one_line": false,
    "luahelper.format.keep_simple_function_one_line": false,
    "luahelper.format.chop_down_table": true,
    "luahelper.format.chop_down_kv_table": true,
    "luahelper.format.single_quote_to_double_quote": true,
    "luahelper.format.spaces_before_call": 1,
    "luahelper.format.extra_sep_at_table_end": true,
    "luahelper.format.spaces_around_equals_in_field": true,
    "luahelper.format.break_before_functioncall_rp": true,
    "luahelper.format.align_parameter": true,
    "luahelper.base.formatErrShow": true,

