    /* -------------------- */
    /* Editor               */
    /* -------------------- */
    "workbench.colorTheme": "Atom One Dark",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "none",
    "editor.fontFamily": "JetBrainsMono Nerd Font",
    "editor.fontSize": 13,
    "editor.lineNumbers": "on",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": "active",
    "editor.cursorSmoothCaretAnimation": "off",
    "editor.minimap.enabled": false,
    "editor.hover.enabled": true,
    "editor.hover.sticky": true,
    "editor.hover.delay": 750,
    "editor.occurrencesHighlight": "off",
    "editor.selectionHighlight": false,
    "editor.matchBrackets": "never",
    "editor.linkedEditing": true,
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.maxLineCount": 5,
    "editor.stickyScroll.defaultModel": "foldingProviderModel",
    "editor.gotoLocation.multipleDefinitions": "goto",

    /* -------------------- */
    /* Terminal             */
    /* -------------------- */
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.tabs.defaultIcon": "terminal-bash",
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.cursorBlinking": true,

    /* -------------------- */
    /* Files                */
    /* -------------------- */
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1,
    "files.associations": {
        "*.css": "tailwindcss",
        "*.scss": "tailwindcss"
    },

    /* -------------------- */
    /* Explorer             */
    /* -------------------- */
    "explorer.confirmDelete": false,
    "explorer.compactFolders": false,
    "explorer.confirmDragAndDrop": false,

    /* -------------------- */
    /* Miscellaneous        */
    /* -------------------- */
    "output.smartScroll.enabled": false,
    "security.workspace.trust.untrustedFiles": "prompt",
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.maxComputationTime": 0,
    "telemetry.telemetryLevel": "off",
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.svg": "default"
    },
    "workbench.colorCustomizations": {
        /* Similar Selection */
        // "editor.selectionHighlightBackground": "#0000",
        // "editor.selectionHighlightBorder": "#0000",
        /* Syntax Variable Assignment */
        // "editor.wordHighlightStrongBackground": "#0000",
        // "editor.wordHighlightStrongBorder": "#0000",
        /* Syntax Variable */
        // "editor.wordHighlightBackground": "#0000",
        // "editor.wordHighlightBorder": "#0000",
        /* Brackets */
        // "editorBracketMatch.border": "#0000",
        // "editorBracketMatch.background": "#0000"
    },

    /* -------------------- */
    /* Git                  */
    /* -------------------- */
    "git.defaultBranchName": "master",
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.postCommitCommand": "none",
    "git.ignoreRebaseWarning": true,

    /* -------------------- */
    /* Codeium              */
    /* -------------------- */
    "codeium.enableCommandSideHintEducation": false,
    "codeium.enableExplainProblem": false,
    "codeium.enableInComments": false,
    "codeium.enableCodeLens": false,
    "codeium.enableConfig": {
        "*": false
    },

    /* -------------------- */
    /* GitHub Copilot       */
    /* -------------------- */
    "github.copilot.editor.enableAutoCompletions": false,
    "github.copilot.editor.enableCodeActions": false,
    "github.copilot.enable": {
        "*": false
    },

    /* -------------------- */
    /* Prettier             */
    /* -------------------- */
    "prettier.requireConfig": false,
    "prettier.jsxSingleQuote": true,
    "prettier.singleQuote": true,
    "prettier.printWidth": 100,
    "prettier.tabWidth": 4,
    "prettier.useTabs": false,

    /* -------------------- */
    /* Live Server          */
    /* -------------------- */
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,

    /* -------------------- */
    /* Code Runner          */
    /* -------------------- */
    "code-runner.enableAppInsights": false,
    "code-runner.executorMap": {
        "javascript": "node",
        "typescript": "ts-node",
        "python": "python -u",
        "lua": "love .",
        "go": "go run",
        "sass": "sass --style expanded",
        "scss": "scss --style expanded",
        "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "zig": "zig run",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "shellscript": "bash",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "ahk": "autohotkey",
        "bat": "cmd /c",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt"
    },

    /* -------------------- */
    /* JavaScript           */
    /* -------------------- */
    "[javascript]": {
        "editor.tabSize": 4,
        "prettier.tabWidth": 4,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",

    /* -------------------- */
    /* TypeScript           */
    /* -------------------- */
    "[typescript]": {
        "editor.tabSize": 4,
        "prettier.tabWidth": 4,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",

    /* -------------------- */
    /* JavaScript React     */
    /* -------------------- */
    "[javascriptreact]": {
        "editor.tabSize": 2,
        "prettier.tabWidth": 2,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    /* -------------------- */
    /* TypeScript React     */
    /* -------------------- */
    "[typescriptreact]": {
        "editor.tabSize": 2,
        "prettier.tabWidth": 2,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    /* -------------------- */
    /* Vue.js               */
    /* -------------------- */
    "[vue]": {
        "editor.tabSize": 2,
        "prettier.tabWidth": 2,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "Vue.volar"
    },

    /* -------------------- */
    /* HTML                 */
    /* -------------------- */
    "[html]": {
        "editor.tabSize": 2,
        "prettier.tabWidth": 2,
        "editor.detectIndentation": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    // "html.hover.documentation": false,
